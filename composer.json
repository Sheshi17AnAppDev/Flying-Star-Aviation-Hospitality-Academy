{
    "name": "flying-star-aviation/php-backend",
    "description": "PHP backend for Flying Star Aviation Academy",
    "type": "project",
    "require": {
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FlyingStar\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
